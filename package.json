    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@playroom/playroom": "^0.0.1",
    "@types/color": "^3.0.0",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@types/styled-system": "^5.1.4",
    "husky": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-use": "^13.10.0",
    "styled-components": "^4.4.1",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "color": "^3.1.2",
    "formik": "^2.0.6",
