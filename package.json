  "license": "MIT",
  "author": "Nikhil Saraf",
  "main": "dist/index.js",
  "module": "dist/react-box.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "git://github.com/nksaraf/react-box.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && npm run playroom:build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint",
    "playroom:deploy": "now --prod --name react-box",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
